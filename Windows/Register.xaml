<Window x:Class="LoginPanel.Windows.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoginPanel.Windows"
        mc:Ignorable="d"
        Title="Rejestracja" Height="230" Width="350">
    <Grid Margin="0,0,0,32">
        <Label Content="Adres Email" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top"/>
        <Label Content="Hasło" HorizontalAlignment="Left" Margin="26,86,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.592,0.575"/>
        <Button x:Name="btn_register" Content="Rejestracja" IsDefault="True" HorizontalAlignment="Left" Margin="87,136,0,0" VerticalAlignment="Top" Width="110" Click="btn_register_Click"/>
        <Button x:Name="btn_cancel" Content="Anuluj" IsCancel="True" HorizontalAlignment="Left" Margin="207,136,0,0" VerticalAlignment="Top" Width="110" Click="Btn_cancel_Click"/>
        <TextBox x:Name="txtbox_email" HorizontalAlignment="Left" Height="23" Margin="87,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="230"/>
        <PasswordBox x:Name="txtbox_password" HorizontalAlignment="Left" Height="23" Margin="87,86,0,0" VerticalAlignment="Top" Width="230"/>

    </Grid>
</Window>
